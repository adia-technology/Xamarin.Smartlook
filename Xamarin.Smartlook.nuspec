<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Xamarin.Smartlook</id>
        <title>Xamarin.Smartlook</title>
        <version>$Version$</version>
        <description>Multiplatform library that makes it easy to start tracking sessions with Smartlook.com in your Xamarin application. It encapsulates both native SDKs provided by Smartlook.</description>
        <authors>rnowosielski</authors>
        <owners>Rafał Nowosielski</owners>
        <projectUrl>https://github.com/adia-technology/Xamarin.Smartlook</projectUrl>
        <license type="expression">MIT</license>     
        <tags>smartlook, xamarin</tags>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <summary>Xamaring client package for Smartlook</summary>
        <copyright>Adia, Just in time staffing AG</copyright>
        <dependencies>
          <group targetFramework="MonoAndroid9.0">
            <dependency id="Square.Retrofit2.ConverterGson" version="2.4.0.1" />
            <dependency id="Xamarin.Kotlin.StdLib.Jdk7" version="1.3.50.1" />
            <dependency id="Xamarin.Kotlin.Reflect" version="1.3.50.1" />
          </group>
          <group targetFramework="Xamarin.iOS1.0" />
          <group targetFramework=".NETStandard2.0" />
        </dependencies>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="Java.Interop" targetFramework="MonoAndroid9.0" />
            <frameworkAssembly assemblyName="Mono.Android" targetFramework="MonoAndroid9.0" />
            <frameworkAssembly assemblyName="System.Core" targetFramework="MonoAndroid9.0" />
            <frameworkAssembly assemblyName="System" targetFramework="MonoAndroid9.0" />
            <frameworkAssembly assemblyName="System.Xml" targetFramework="MonoAndroid9.0" />
            <frameworkAssembly assemblyName="System.Core" targetFramework="Xamarin.iOS1.0" />
            <frameworkAssembly assemblyName="System" targetFramework="Xamarin.iOS1.0" />
            <frameworkAssembly assemblyName="System.Xml" targetFramework="Xamarin.iOS1.0" />
            <frameworkAssembly assemblyName="Xamarin.iOS" targetFramework="Xamarin.iOS1.0" />
        </frameworkAssemblies>
   </metadata>
    <files>
        <file src="Xamarin.Smartlook.iOS\bin\$Configuration$\*.dll" target="lib\xamarinios10" />
        <file src="Xamarin.Smartlook.Android\bin\$Configuration$\*.dll" target="lib\monoandroid90" />
        <file src="Xamarin.Smartlook\bin\$Configuration$\netstandard2.0\*.dll" target="lib\netstandard2.0" />
    </files>
   
</package>